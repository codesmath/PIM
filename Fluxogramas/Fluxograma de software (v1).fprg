                    <output expression="&quot;N&#227;o podemos identificar sua resposta! Revise e digite novamente, conforme o menu.&quot;" newline="True"/>
                    <input variable="servi&#231;o"/>
                    <while expression="servi&#231;o&lt;1">
                        <output expression="&quot;N&#227;o podemos identificar sua resposta! Revise e digite novamente, conforme o menu.&quot;" newline="True"/>
                        <input variable="servi&#231;o"/>
                    </while>
                </while>
            </do>
            <output expression="&quot;Obrigado pela prefer&#234;ncia!&quot;" newline="True"/>
        </body>
