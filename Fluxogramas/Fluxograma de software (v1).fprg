    <function name="cliente" type="None" variable="">
        <parameters/>
        <body>
            <comment text="fluxo de cliente"/>
            <declare name="servi&#231;o" type="Integer" array="False" size=""/>
            <comment text="variaveis de loguin"/>
            <declare name="nome" type="String" array="False" size=""/>
