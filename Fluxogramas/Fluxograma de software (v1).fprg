                <if expression="servi&#231;o=1">
                    <then>
                        <comment text="aluguel"/>
                        <call expression="aluguel"/>
                    </then>
                    <else>
                        <comment text="devolu&#231;&#227;o"/>
                        <call expression="devolu&#231;&#227;o"/>
                    </else>
                </if>
