                <input variable="usuario"/>
            </while>
            <declare name="cpf" type="Integer" array="False" size=""/>
            <declare name="nome" type="String" array="False" size=""/>
            <if expression="usuario=1">
                <then>
                    <comment text="Fluxo de funcion&#225;rio"/>
                    <call expression="Funcionario"/>
                </then>
                <else>
                    <comment text="fluxo de cliente"/>
                    <output expression="&quot;Por favor, informe seu CPF.&quot;" newline="True"/>
                    <input variable="cpf"/>
